By 030902414, contest: Codeforces Beta Round #89 (Div. 2), problem: (C) Fancy Number, Accepted, #
 #include<iostream>
#include<cstdio>
#include<cstring>
#include<queue>
#include<vector>
#include<cmath>
#include<functional>
using namespace std;
const int inf=0x3fffffff;
typedef struct pnode {
    int pos, d;

    pnode() {
    }

    pnode(int pos1, int d1) : pos(pos1), d(d1) {
    }

    bool operator<(const pnode & z)const {
        if (abs(d) == abs(z.d)){
            if(d==0)
                return 0;
           if(pos<z.pos){
               if(d<0)
                return 0;
            else
                return 1;
           }
           else{
               if(d<0 && z.d>0)
                   return 0;
               else if(d>0 && z.d>0)
                   return 0;
               return 1;
                   

           }
        }
        return abs(d) > abs(z.d);
    }
} pnode;

int main() {
   
    int n, k, i, j, d,minn;
    string s, ss;
    int ans[10];
    vector<int> path[10];

    while (cin >> n >> k >> s) {
        minn=inf;
        for (i = 0; i < 10; ++i) {

            
            priority_queue<pnode, vector<pnode> > Q;
            for (j = 0; j < n; ++j) {
                d = i - s[j]+'0';
                Q.push(pnode(j, d));

            }
            ans[i] = 0;
            path[i].clear();
            for (j = 0; j < k; ++j) {
                d = Q.top().d;
                ans[i] += abs(d);

                path[i].push_back(Q.top().pos);
                Q.pop();
            }
           minn=minn<=ans[i]?minn:ans[i];
        }
        priority_queue<string,vector<string> ,greater<string> > qs;
        for(i=0;i<10;++i){
            if(minn==ans[i]){
                ss=s;
                for(j=0;j<path[i].size();++j){
                    ss[path[i][j]]=i+'0';

                }
           //     cout<<"ss= "<<ss<<endl;
                qs.push(ss);
            }
        }
        cout<<minn<<endl<<qs.top()<<endl;

    }

    return 0;
}


